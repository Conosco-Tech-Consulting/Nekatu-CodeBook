C:\Users\Administrator\Documents\Projects\UACT>IF NOT "C:\Users\Administrator\Documents\Projects\UACT\bulk.csv" == "" (
CALL :BULKCHECK C:\Users\Administrator\Documents\Projects\UACT\bulk.csv  
 GOTO :EXIT 
) 

C:\Users\Administrator\Documents\Projects\UACT>FOR /F "tokens=1-10 delims=," %a in (C:\Users\Administrator\Documents\Projects\UACT\bulk.csv) do (
CALL :BANNER  
 ECHO. 
 ECHO Bulk User Mode  
 ECHO. 
 SET FNAME=%a  
 SET LNAME=%b  
 SET UNAME=%c  
 SET UPSWD=%d  
 SET ADORG=%e  
 SET ADGRP=%f  
 SET INTERN=%g  
 SET EXMAIL=%h  
 SET EXADDR=%i  
 SET EXSTOR=%j  
 ECHO. 
 ECHO - Full Name:	   
 ECHO - Department:	  
 ECHO - Intern User:	  
 ECHO. 
 ECHO - User Name:	  
 ECHO - Password:	  
 ECHO. 
 ECHO - Mailbox:	  
 IF DEFINED ExADDR ECHO - Email:	  
 ECHO. 
 CALL :CREATE  
 SET COUNT=+1  
 ECHO  User(s) Created 
) 

