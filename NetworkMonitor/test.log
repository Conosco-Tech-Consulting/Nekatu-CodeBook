c:\inetpub\wwwroot\resources\Checkup>for /F "tokens=2 delims=," %a in (hosts.csv) do (
echo Checking %a...  
 echo <tr><td>%a</td>  1>>hosts.htm  
 for /F "tokens=6" %x in ('PortQry.exe -n %a -e 3389 | find /i "TCP"') do (
if /I "%x" == "NOT" echo <td><font color="red">DOWN</font></td>  1>>hosts.htm  
 if /I "%x" == "LISTENING" echo <td><font color="green">LIVE</font></td>  1>>hosts.htm  
 if /I "%x" == "TIMEOUT" echo <td><font color="orange">UNKNOWN</font></td>  1>>hosts.htm  
